{
  "name": "server",
  "module": "index.ts",
  "scripts": {
    "migrate": "bun -b kysely-migrate",
    "db:start": "docker-compose up -d",
    "db:stop": "docker-compose down",
    "db:shutdown": "powershell.exe -Command \"Stop-Process -Name 'Docker Desktop'\" && wsl --shutdown",
    "start": "bun --watch run src/server.ts",
    "build": "bun build ./src/server.ts ./src/**/*.ts --outdir dist --target node"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "crypto": "^1.0.1",
    "express": "^4.19.2",
    "express-fileupload": "^1.5.0",
    "express-rate-limit": "^7.2.0",
    "google-auth-library": "^9.10.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "kysely": "^0.27.3",
    "mysql2": "^3.9.7",
    "nodemailer": "^6.9.13",
    "socket.io": "^4.7.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/bun": "latest",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.12",
    "@types/nodemailer": "^6.4.15",
    "kysely-migrate": "^0.0.16",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
}